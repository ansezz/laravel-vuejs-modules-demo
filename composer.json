{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "./packages/ansezz/laravel-vuejs-modules",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "ansezz/laravel-vuejs-modules": "dev-master",
    "barryvdh/laravel-cors": "^0.11.0",
    "fideloper/proxy": "~4.0",
    "folklore/graphql": "^1.0",
    "laravel/framework": "5.6.*",
    "laravel/passport": "^5.0",
    "laravel/tinker": "~1.0",
    "league/fractal": "^0.17.0",
    "prettus/l5-repository": "^2.6"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "nunomaduro/collision": "~1.1",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0",
    "symfony/thanks": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Ansezz\\Modules\\": "src/",
      "Modules\\": "Modules/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ansezz\\Modules\\LaravelVuejsModulesServiceProvider"
      ],
      "aliases": {
        "Modules": "Ansezz\\Modules\\Facades"
      },
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
